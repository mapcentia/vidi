                                                            <option value="2">{{X-thin}}</option>
                                                            <option value="3">{{Thin}}</option>
                                                            <option value="4" selected>{{Medium}}</option>
                                                            <option value="5">{{Thick}}</option>
                                                            <option value="6">{{X-Thick}}</option>
                                                        </select>
