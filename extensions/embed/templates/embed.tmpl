<style>
    #list {
        position: absolute;
        height: 100%;
        right: 0;
        left: 0;
        z-index: 1;
        background-color: #ffffff;
        display: none;
    }

    #switcher, #switcher-list {
        height: 50px;
        top: 5px;
        z-index: 2;
        display: block;
        margin: auto;
        position: absolute;
        left: 0;
        right: 0;
    }

    #switcher {
        width: 200px;

    }

    #switcher-list {
        top: 50px;
