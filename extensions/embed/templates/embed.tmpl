    }

    .fixed-table-container {
        height: calc(100vh - 100px) !important;
    }

    .list-group .list-group-item {
        padding: 3px 16px;
    }

    .modal-dialog br {
        line-height: 28px;
    }

    .carousel {
        margin-top: 5px;
    }

    .vmus-text {
        margin-top: 28px;
    }

    #share-buttons {
        margin-top: 10px;
        margin-buttom: 10px;
    }

    img.center, div.center {
        display: block;
        margin: 0 auto;
        width: 230px;
        margin-bottom: 20px;
    }

    .start-list-icon {
        margin-left: -17px;
        width: 35px;
        height: 45px;
        transform: none;
        z-index: 121;
        position: relative;
        display: inline-block;
        vertical-align: middle;
    }

    .start-list-text {
        position: relative;
        margin-left: 14px;
        max-width: 85%;
        display: inline-block;
        vertical-align: middle;

    }

    .start-list-text p {
        margin: 0;
    }

    .nav .checkbox {
        width: 140px;
        display: inline-block;
        height: 50px;
        padding-top: 12px;
    }

    .nav .overlay-label {
        width: 130px;
        font-size: 18px;
        font-weight: 300;
    }

    #burger-btn {
        display: inline;
    }

    #info-modal {
        display: inline;
    }

    #vectorlayers .panel:first-child {
        display: none;
    }

    #count-badge {

        position: absolute;
        right: 16px;
        top: -2px;
        font-size: 10pt;
    }

    @media (max-width: 767px) {
        .dialog-bottom {
            min-width: 100%;
            border-radius: 0;
            margin-bottom: 0;
            right: 0;
        }

        .dialog-bottom .modal-body {
            max-height: 60vh;

        }

        .navbar-brand.navbar-brand-text {
            display: none;
        }

        .nav .checkbox {
            display: none;
        }

    }

    @media (min-width: 1200px) {
        .navbar-toggle {
            margin-top: 14px;
        }

        .nav .checkbox {
            padding-top: 17px;
        }

    }

    @media (max-width: 1200px) {

        #floating-container-primary {
            top: 58px;

        }

        #floating-container-secondary {
            top: 225px;

        }

        .navbar-brand {
            height: 50px;
            padding: 10px 0px;
        }
    }

    .btn:not(.btn-raised):not(.btn-link):focus, .btn:not(.btn-raised):not(.btn-link):hover, .input-group-btn .btn:not(.btn-raised):not(.btn-link):focus, .input-group-btn .btn:not(.btn-raised):not(.btn-link):hover {
        background-color: #EEEEEE;
    }

    .container {
        width: 100%;
        margin: 0;
    }

    .layer-count.badge {
        display: none;
    }

    .nav .checkbox input[type=checkbox]:checked + .checkbox-material .check, label.checkbox-inline input[type=checkbox]:checked + .checkbox-material .check {
        color: #fff;
        border-color: #fff;
        background-color: #fff;

    }

    .nav .checkbox .checkbox-material .check, label.checkbox-inline .checkbox-material .check {
        border: 2px solid #bdbdbd;
    }

    .form-group.is-focused .checkbox label, .form-group.is-focused label.checkbox-inline {
        color: #bdbdbd !important;
    }

    .panel-heading a, .panel-heading a:focus, .panel-heading a:hover {

        text-decoration: none;
    }

    #btn-start {
        font-size: 18px;
        margin-right: 30px;
    }

    .refresh-vector-layer {
        display: none !important;
    }
</style>
