        -webkit-border-top-left-radius: 5px;
        -moz-border-radius-bottomleft: 5px;
        -moz-border-radius-topleft: 5px;
        border-bottom-left-radius: 5px;
        border-top-left-radius: 5px;
    }

    #btn-list, #btn-list-alpha {
        float: right;
        -webkit-border-bottom-right-radius: 5px;
        -webkit-border-top-right-radius: 5px;
        -moz-border-radius-bottomright: 5px;
        -moz-border-radius-topright: 5px;
        border-bottom-right-radius: 5px;
        border-top-right-radius: 5px;
    }

    #btn-kort, #btn-list {
        width: 50%;
        text-align: center;
        background-color: rgba(224, 241, 251, 0.9);

    }

    #btn-list-dis, #btn-list-alpha {
        width: 50%;
        text-align: center;
        background-color: rgba(241, 241, 241, 0.9);

    }

    .nav > li {
        float: left !important;
    }

    #switcher .active {
        background-color: #008ECF;
        color: white;
    }

    #switcher-list .active {
        background-color: #9e9e9e;
        color: white;
    }

    #switcher-list .disabled {
        color: white;
        cursor: not-allowed;
    }

    #btn-list h3, #btn-kort h3 {
        top: 0;
        left: 0;
        bottom: 0;
        right: 0;
        margin-top: 0 !important;
        margin-bottom: 0 !important;
        padding: 5px;
        font-size: 14px;
        font-weight: 700;
        cursor: pointer;
    }

    #btn-list-dis h3, #btn-list-alpha h3 {
        top: 0;
        left: 0;
        bottom: 0;
        right: 0;
        margin-top: 0 !important;
        margin-bottom: 0 !important;
        padding: 3px;
        font-size: 12px;
        font-weight: 300;
        cursor: pointer;
    }

    .naturpark-list-item {
        font-weight: 300;
        cursor: pointer;
        text-align: left;
        width: 100%;
        margin: 2px 0 0 0;
        background-color: rgba(248, 248, 248, 1);
    }

    .naturpark-list-item div {
        white-space: nowrap;
    }

    .navbar {
        left: 0 !important;
    }

    .leaflet-control-locate {
        margin-top: 0;
        margin-bottom: 0;
        box-shadow: none;
        z-index: 0;
    }

    .btn-share {
        margin: 5px !important;
    }

    .leaflet-top.leaflet-right {
        display: none !important;
    }

    .distance {
        font-size: 80%;
        vertical-align: middle;
        line-height: 0;
        margin-right: 10px;
        color: #9e9e9e;
    }

    .btn-text {
        margin-bottom: 7px;
    }

    .btn-text-dis {
        margin-top: 7px;
    }

    .btn-text-alpha {
        margin-top: 12px;
    }

    .carousel-caption {
        padding: 0;
        top: 10px;
    }

    .embed-responsive {
        margin-top: 18px;
    }

    #main {
        margin: 0 auto;
        text-align: left;
    }

    ul {
        list-style: none;
    }

    #start-modal ul {
        padding-left: 5px;
    }

    #start-modal li {
        margin-bottom: 7px;
    }

    .form-inline .form-control {
        width: 50%;
        margin-left: 80px;
    }

    .done {
        color: #00ff33;
        text-decoration: line-through;
    }

    .undone {
        color: #ff0033;
    }

    .icon {
        margin: 6px 10px 6px 0;
    }

    .list-group-item {
        padding: 9px 3px;

        border: none;
    }

    .inherit-size {
        font-size: inherit;
    }

    #floating-container-primary {
        position: absolute;
        top: 68px;
        right: 8px;
        width: 40px;
    }

    #floating-container-secondary {
        position: absolute;
        top: 235px;
        right: 8px;
        width: 40px;
    }

    #floating-container-secondary a {
        margin-bottom: 8px;
    }

    #floating-container-primary a {
        margin-bottom: 8px;
    }

    .leaflet-control-locate a, .leaflet-control-locate a:hover {
        border-radius: 50% !important;
        font-size: 24px;
        height: 40px;
        margin: auto;
        min-width: 40px;
        width: 40px;
        padding: 0;
        overflow: hidden;
        -webkit-box-shadow: 0 1px 1.5px 0 rgba(0, 0, 0, 0.12), 0 1px 1px 0 rgba(0, 0, 0, 0.24);
        box-shadow: 0 1px 1.5px 0 rgba(0, 0, 0, 0.12), 0 1px 1px 0 rgba(0, 0, 0, 0.24);
        position: relative;
        line-height: normal;
        line-height: 40px
    }

    .btn.btn-fab btn-fab-mini, .btn.btn-fab btn-fab-mini.btn-primary, .btn:not(.btn-raised):not(.btn-link):hover {

    }

    .dialog-bottom {
        padding: 14px 15px;
        margin-bottom: 20px;
        height: auto;

        clear: both;
        min-width: 288px;
        max-width: 568px;

        position: fixed;
        right: 56px;
        bottom: -100%;

